msgstr "IP 日志阅读器"

msgid "IPアドレスログ"
msgstr "IP 地址日志"

msgid "全クリア"
msgstr "一切正常"

msgid "IPログが全て削除されました"
msgstr "所有 IP 日志均已删除。"

msgid "ID"
msgstr "身份证"

msgid "タイムスタンプ"
msgstr "时间戳"

msgid "IPアドレス"
msgstr "IP 地址"

msgid "ユーザーエージェント"
msgstr "用户代理"

msgid "イベント"
msgstr "活动"

